{
  "name": "spiffy-public-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn clean\" \"next dev\" \"yarn css-types\"",
    "css-types": "tsm \"**/*.module.scss\" -w -u -e default -q double --ignore \"node_modules\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --quiet --fix",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "bootstrap": "^5.1.3",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-bootstrap": "^2.0.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "concurrently": "^5.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "sass": "^1.29.0",
    "typed-scss-modules": "^4.0.0",
    "typescript": "4.4.4"
  }
}
